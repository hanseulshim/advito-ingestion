{
	"name": "advito-ingestion",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@apollo/client": "^3.0.0-beta.36",
		"@apollo/link-error": "^2.0.0-beta.3",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"antd": "^4.0.0",
		"babel-plugin-import": "^1.13.0",
		"customize-cra": "^0.9.1",
		"graphql": "^14.5.8",
		"isomorphic-unfetch": "^3.0.0",
		"less": "^3.10.3",
		"less-loader": "^5.0.0",
		"moment": "^2.24.0",
		"moment-timezone": "^0.5.27",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-app-rewired": "^2.1.5",
		"react-dom": "^16.12.0",
		"react-router-dom": "^5.1.2",
		"react-scripts": "3.3.0",
		"styled-components": "^5.0.0",
		"xlsx": "^0.15.5"
	},
	"scripts": {
		"start": "react-app-rewired start",
		"start:dev": "REACT_APP_STAGE=dev react-app-rewired start",
		"start:staging": "REACT_APP_STAGE=staging react-app-rewired start",
		"start:prod": "REACT_APP_STAGE=prod react-app-rewired start",
		"build": "react-app-rewired build",
		"test": "react-app-rewired test",
		"server": "cd ./server && yarn start",
		"server:staging": "cd ./server && yarn start:staging",
		"server:prod": "cd ./server && yarn start:prod",
		"eject": "react-app-rewired eject",
		"deploy:dev:client": "REACT_APP_STAGE=dev react-app-rewired build && aws s3 sync  ./build s3://dev.boostlabs/advito-ingestion/ --acl public-read --profile boost",
		"deploy:dev:server": "cd ./server && yarn dev",
		"deploy:dev": "yarn deploy:dev & yarn deploy:dev:server",
		"deploy:staging:client": "REACT_APP_STAGE=staging react-app-rewired build && aws s3 sync  ./build s3://staging.boostlabs/advito-ingestion/ --acl public-read --profile boost",
		"deploy:staging:server": "cd ./server && yarn staging",
		"deploy:staging": "yarn deploy:staging & yarn deploy:staging:server",
		"deploy:prod:client": "REACT_APP_STAGE=prod react-app-rewired build && aws s3 sync  ./build s3://advito-ingestion/ --acl public-read --profile advito",
		"deploy:prod:server": "cd ./server && yarn prod",
		"deploy:prod": "yarn deploy:prod & yarn deploy:prod:server",
		"deploy:release:client": "yarn deploy:dev:client && yarn deploy:staging:client",
		"deploy:release:server": "yarn deploy:dev:server && yarn deploy:staging:server",
		"deploy:release": "yarn deploy:dev && yarn deploy:staging"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"homepage": "."
}

{
  "name": "advito-ingestion",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.36",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.0.0",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^0.9.1",
    "graphql": "^14.5.8",
    "isomorphic-unfetch": "^3.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "styled-components": "^5.0.0",
    "xlsx": "^0.15.5"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "start:alpha": "REACT_APP_STAGE=alpha react-app-rewired start",
    "start:beta": "REACT_APP_STAGE=beta react-app-rewired start",
    "start:dev": "REACT_APP_STAGE=dev react-app-rewired start",
    "start:prod": "REACT_APP_STAGE=prod react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "server": "cd ./server && yarn start",
    "server:prod": "cd ./server && yarn start:prod",
    "eject": "react-app-rewired eject",
    "deploy:alpha": "REACT_APP_STAGE=alpha react-app-rewired build && aws s3 sync  ./build s3://alpha.boostlabs/advito-ingestion/ --acl public-read --profile boost",
    "deploy:alpha:server": "cd ./server && yarn alpha",
    "deploy:alpha:all": "yarn deploy:alpha & yarn deploy:alpha:server",
    "deploy:beta": "REACT_APP_STAGE=beta react-app-rewired build && aws s3 sync  ./build s3://beta.boostlabs/advito-ingestion/ --acl public-read --profile boost",
    "deploy:beta:server": "cd ./server && yarn beta",
    "deploy:beta:all": "yarn deploy:beta & yarn deploy:beta:server",
    "deploy:dev": "REACT_APP_STAGE=dev react-app-rewired build && aws s3 sync  ./build s3://dev.boostlabs/advito-ingestion/ --acl public-read --profile boost",
    "deploy:dev:server": "cd ./server && yarn dev",
    "deploy:dev:all": "yarn deploy:dev & yarn deploy:dev:server",
    "deploy:prod": "REACT_APP_STAGE=prod react-app-rewired build && aws s3 sync  ./build s3://advito-ingestion/ --acl public-read --profile advito",
    "deploy:prod:server": "cd ./server && yarn prod",
    "deploy:prod:all": "yarn deploy:prod & yarn deploy:prod:server",
    "deploy:all:front": "yarn deploy:alpha && yarn deploy:beta && yarn deploy:dev && yarn deploy:prod",
    "deploy:all:server": "cd ./server && yarn all",
    "deploy:all": "yarn deploy:alpha:all && yarn deploy:beta:all && yarn deploy:dev:all && yarn deploy:prod:all"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "."
}
